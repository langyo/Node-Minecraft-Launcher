{
    "extends": [
        "standard"
    ],
    "installedESLint": true,
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "parser": "babel-eslint",
    "plugins": [
        "standard",
        "promise"
    ]
}